{
  "hosting": {
    "public": "out",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "cleanUrls": true,
    "rewrites": [
      {
        "source": "**",
        "destination": "index.html"
      }
    ]
  },
  "functions": {
    "source": ".",
    "predeploy": [
      "npm run build-functions"
    ],
    "runtime": "nodejs12"
  }
}